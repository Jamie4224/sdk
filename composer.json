{
  "name": "myparcelnl/sdk",
  "version": "7.2.2",
  "description": "This package is designed to send and receive data from MyParcel by means of an API.",
  "keywords": [
    "MyParcel",
    "Flespakket",
    "PostNL"
  ],
  "homepage": "https://myparcelnl.github.io/sdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Edie Lemoine",
      "email": "support@myparcel.nl"
    },
    {
      "name": "Joeri van Veen",
      "email": "support@myparcel.nl"
    },
    {
      "name": "Mark Ernst",
      "email": "support@myparcel.nl"
    },
    {
      "name": "Richard Perdaan",
      "email": "support@myparcel.nl"
    }
  ],
  "require": {
    "php": ">=7.1.0"
  },
  "require-dev": {
    "fakerphp/faker": "^v1.16.0",
    "pestphp/pest": "^v1.21",
    "pestphp/pest-plugin-faker": "^1.x-dev",
    "pestphp/pest-plugin-watch": "^1.x-dev"
  },
  "autoload": {
    "psr-4": {
      "MyParcelNL\\Sdk\\src\\": "src",
      "MyParcelNL\\Sdk\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MyParcelNL\\Sdk\\Test\\": "test"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
